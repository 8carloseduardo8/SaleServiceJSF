	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>faleConosco</managed-bean-name>
		<managed-bean-class>controller.FaleConoscoController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>cadastroAnuncio</managed-bean-name>
		<managed-bean-class>controller.CadastroAnuncioController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>consultarAnuncio</managed-bean-name>
		<managed-bean-class>controller.ConsultarAnuncioController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>visualizarAnuncio</managed-bean-name>
		<managed-bean-class>controller.VisualizarAnuncio</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>

